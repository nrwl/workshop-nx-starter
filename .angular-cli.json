{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "tuskdesk-suite",
    "npmScope": "tuskdesk-suite",
    "latestMigration": "20180405-add-workspace-schematic-command"
  },
  "apps": [
    {
      "name": "customer-portal",
      "root": "apps/customer-portal/src",
      "outDir": "dist/apps/customer-portal",
      "assets": [
        {
          "input": "../../../libs/shared-assets/",
          "output": "assets/",
          "glob": "**/*"
        },
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "tags": []
    },
    {
      "name": "logs",
      "root": "apps/logs/src",
      "outDir": "dist/apps/logs",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "reporting",
      "root": "apps/reporting/src",
      "outDir": "dist/apps/reporting",
      "assets": [
        {
          "input": "../../../libs/shared-assets/",
          "output": "assets/",
          "glob": "**/*"
        },
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "tags": []
    },
    {
      "name": "authentication-state",
      "appRoot": "",
      "root": "libs/authentication-state/src",
      "test": "../../../test.js",
      "tags": []
    },
    {
      "name": "backend",
      "appRoot": "",
      "root": "libs/backend/src",
      "test": "../../../test.js",
      "tags": []
    },
    {
      "name": "data-models",
      "appRoot": "",
      "root": "libs/data-models/src",
      "test": "../../../test.js",
      "tags": []
    },
    {
      "name": "logs-backend",
      "root": "libs/logs-backend/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "logs-view",
      "root": "libs/logs-view/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "reporting-dashboard-view",
      "root": "libs/reporting-dashboard-view/src",
      "test": "../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "ticket-list-view",
      "appRoot": "",
      "root": "libs/ticket-list-view/src",
      "test": "../../../test.js",
      "tags": []
    },
    {
      "name": "tickets-state",
      "appRoot": "",
      "root": "libs/tickets-state/src",
      "test": "../../../test.js",
      "tags": []
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/customer-portal/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/customer-portal/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/reporting/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/reporting/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/logs/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/logs/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "schematics": {
      "collection": "@nrwl/schematics",
      "postGenerate": "npm run format",
      "newProject": [
        "app",
        "lib"
      ]
    },
    "styleExt": "scss",
    "component": {}
  },
  "warnings": {
    "typescriptMismatch": false
  }
}
