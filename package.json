{
  "name": "tuskdesk-suite",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "legacy-agent-workdesk": "live-server ./legacy/agent-workdesk/app --host=localhost --port=8000 --entry-file=index.html --quiet",
    "server": "tsc -p ./server && copyfiles ./server/server-help.html ./dist/server --flat && node dist/server/server.js",
    "server:compile": "tsc -p ./server && copyfiles ./server/server-help.html ./dist/server --flat",
    "server:throttled": "tsc -p ./server && node dist/server/server.js --throttle",
    "server:test": "jest server",
    "reporting": "ng serve reporting --proxy-config=proxy.config.json --port=4202",
    "customer-portal": "ng serve customer-portal --proxy-config=proxy.config.json --port=4203",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/cdk": "^7.0.2",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/flex-layout": "angular/flex-layout-builds",
    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/material": "^7.0.2",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/platform-server": "^7.0.1",
    "@angular/router": "^7.0.1",
    "@angular/service-worker": "^7.0.1",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@nrwl/nx": "^7.0.0",
    "core-js": "^2.5.7",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.3",
    "@angular/cli": "^7.0.3",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/language-service": "^7.0.1",
    "@ngrx/schematics": "^6.1.0",
    "@ngrx/store-devtools": "6.0.1",
    "@nrwl/builders": "^7.0.0",
    "@nrwl/schematics": "^7.0.0",
    "@types/jest": "^23.0.0",
    "@types/node": "~8.9.4",
    "body-parser": "1.18.2",
    "codelyzer": "^4.5.0",
    "copyfiles": "^2.0.0",
    "cors": "2.8.0",
    "express": "4.16.2",
    "hammerjs": "2.0.8",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.1",
    "live-server": "^1.2.0",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "^1.14.3",
    "protractor": "^5.4.1",
    "resolve": "^1.7.1",
    "supertest": "^3.1.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
